{%- if section.settings.hidden %}{% break %}{% endif -%}
{% assign se_blocks = section.blocks %}
<div class="apus-footer-mobile">
  <ul>
    {%- for block in section.blocks -%}
      {%- case block.type -%}
        {%- when 'home' -%}
          <li>
            <a href="{{ routes.root_url }}" aria-label="link">
              <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M12 18V15" stroke="#212529" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M10.0698 2.82L3.13978 8.37C2.35978 8.99 1.85978 10.3 2.02978 11.28L3.35978 19.24C3.59978 20.66 4.95978 21.81 6.39978 21.81H17.5998C19.0298 21.81 20.3998 20.65 20.6398 19.24L21.9698 11.28C22.1298 10.3 21.6298 8.99 20.8598 8.37L13.9298 2.83C12.8598 1.97 11.1298 1.97 10.0698 2.82Z" stroke="#212529" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
              <span>{{ 'homepage.title' | t }}</span>
            </a>
          </li>
        {%- when 'collections' -%}
          <li>
            <a href="{{ block.settings.btn_link}}" aria-label="link">
              <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M3.00977 11.22V15.71C3.00977 20.2 4.80977 22 9.29977 22H14.6898C19.1798 22 20.9798 20.2 20.9798 15.71V11.22" stroke="#2B38D1" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M12 12C13.83 12 15.18 10.51 15 8.68L14.34 2H9.66999L8.99999 8.68C8.81999 10.51 10.17 12 12 12Z" stroke="#2B38D1" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M18.3098 12C20.3298 12 21.8098 10.36 21.6098 8.35L21.3298 5.6C20.9698 3 19.9698 2 17.3498 2H14.2998L14.9998 9.01C15.1698 10.66 16.6598 12 18.3098 12Z" stroke="#2B38D1" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M5.63988 12C7.28988 12 8.77988 10.66 8.93988 9.01L9.15988 6.8L9.63988 2H6.58988C3.96988 2 2.96988 3 2.60988 5.6L2.33988 8.35C2.13988 10.36 3.61988 12 5.63988 12Z" stroke="#2B38D1" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M12 17C10.33 17 9.5 17.83 9.5 19.5V22H14.5V19.5C14.5 17.83 13.67 17 12 17Z" stroke="#2B38D1" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
              <span>{{ 'homepage.shop' | t }}</span>
            </a>
          </li>
        {%- when 'cart' -%}
          <li class="cart_mobile">
            <a
              class="footer-mini-cart mini-cart push_side header-icon"
              href="{{ routes.cart_url }}"
              data-id="#js_cart_popup"
              aria-label="link"
            >
              <svg width="26" height="22" viewBox="0 0 26 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M18.6557 19.8526C18.3711 19.8526 18.0982 19.7398 17.897 19.5389C17.6957 19.3379 17.5827 19.0654 17.5827 18.7813H15.4365C15.4365 19.6337 15.7757 20.4512 16.3794 21.0539C16.9831 21.6567 17.8019 21.9953 18.6557 21.9953C19.5095 21.9953 20.3283 21.6567 20.9321 21.0539C21.5358 20.4512 21.8749 19.6337 21.8749 18.7813H19.7288C19.7288 19.0654 19.6157 19.3379 19.4145 19.5389C19.2133 19.7398 18.9403 19.8526 18.6557 19.8526Z" fill="white"/>
                <path d="M11.1048 19.8526C10.8202 19.8526 10.5472 19.7398 10.346 19.5389C10.1447 19.3379 10.0317 19.0654 10.0317 18.7813H7.87549C7.87549 19.204 7.95888 19.6225 8.12089 20.013C8.28291 20.4036 8.52038 20.7584 8.81975 21.0573C9.11912 21.3561 9.47452 21.5932 9.86566 21.755C10.2568 21.9167 10.676 22 11.0994 22C11.5228 22 11.942 21.9167 12.3331 21.755C12.7243 21.5932 13.0797 21.3561 13.379 21.0573C13.6784 20.7584 13.9159 20.4036 14.0779 20.013C14.2399 19.6225 14.3233 19.204 14.3233 18.7813H12.1772C12.1772 19.0653 12.0642 19.3377 11.8631 19.5386C11.662 19.7395 11.3892 19.8525 11.1048 19.8526V19.8526Z" fill="white"/>
                <path d="M25.1035 2.12755C25.045 2.11761 24.9858 2.11268 24.9264 2.11282H7.75732C7.47272 2.11282 7.19978 2.22569 6.99854 2.4266C6.7973 2.62752 6.68425 2.90001 6.68425 3.18415C6.68425 3.46828 6.7973 3.74078 6.99854 3.94169C7.19978 4.1426 7.47272 4.25548 7.75732 4.25548H23.6595L23.3758 5.95487L21.8715 14.9688H7.87334L4.22491 5.95487L2.04859 0.62501C1.9335 0.374275 1.72598 0.177515 1.46924 0.0756873C1.2125 -0.0261399 0.926318 -0.0251882 0.670263 0.0783445C0.414208 0.181877 0.208012 0.380014 0.0945983 0.631508C-0.0188156 0.883003 -0.0307089 1.16848 0.0613942 1.42851L3.53613 9.94089L5.90761 16.2162C6.08266 16.7445 6.44415 17.1114 6.96593 17.1114H22.7803C23.0344 17.1115 23.2803 17.0216 23.4742 16.8577C23.6681 16.6938 23.7975 16.4664 23.8393 16.2162L25.5522 5.95487L25.9854 3.36025C26.0322 3.08003 25.9655 2.79274 25.8001 2.56158C25.6347 2.33041 25.3842 2.17429 25.1035 2.12755V2.12755Z" fill="white"/>
              </svg>

              <span class="header__counter js-cart-count cart-count" data-js-cart-count="{{ cart.item_count }}">
                {{- cart.item_count -}}
              </span>
              <span>{{ 'cart.general.title' | t }}</span>
            </a>
          </li>
        {%- when 'search' -%}
          <li>
            <a class="footer-search-btn push_side" href="#" data-id="#search_pupop" aria-label="link">
              <i class="icon-search"></i>
              <span>{{ 'general.search.btn' | t }}</span>
            </a>
          </li>
        {%- when 'account' -%}
          <li>
            <a href="{{ routes.account_url }}" aria-label="link">
              <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M12.1202 12.78C12.0502 12.77 11.9602 12.77 11.8802 12.78C10.1202 12.72 8.72021 11.28 8.72021 9.51001C8.72021 7.70001 10.1802 6.23001 12.0002 6.23001C13.8102 6.23001 15.2802 7.70001 15.2802 9.51001C15.2702 11.28 13.8802 12.72 12.1202 12.78Z" stroke="#212529" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M18.7398 19.38C16.9598 21.01 14.5998 22 11.9998 22C9.39977 22 7.03977 21.01 5.25977 19.38C5.35977 18.44 5.95977 17.52 7.02977 16.8C9.76977 14.98 14.2498 14.98 16.9698 16.8C18.0398 17.52 18.6398 18.44 18.7398 19.38Z" stroke="#212529" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M12 22C17.5228 22 22 17.5228 22 12C22 6.47715 17.5228 2 12 2C6.47715 2 2 6.47715 2 12C2 17.5228 6.47715 22 12 22Z" stroke="#212529" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
              <span>{{ 'customer.account.title' | t }}</span>
            </a>
          </li>
        {%- when 'wishlist' -%}
          <li>
            <a
              href="#"
              aria-label="link"
              class="push_side header-icon wishlist-icon"
              data-id="#wishlist_popup"
              title="{{'general.wishlist.title' | t}}"
            >
              <i class="icon-heart"></i>
              <span class="header__counter count" data-js-wishlist-count="0">0</span>
              <span>{{ 'general.wishlist.title' | t }}</span>
            </a>
          </li>
      {%- endcase -%}
    {%- endfor -%}
  </ul>
</div>
{%- render 'section_style' -%}
{%- schema -%}
{
  "name": "Menu bottom",
  "class": "menu-bottom",
  "max_blocks": 10,
  "settings": [
  {
  "type": "checkbox",
  "id": "hidden",
  "label": "Hidden Section?",
  "default": false
  }
  ],
  "blocks": [
    {
      "type": "home",
      "name": "Home",
      "limit":1
    },
    {
      "type": "collections",
      "name": "Collections",
      "limit":1,
      "settings": [
      {
      "id": "collection",
      "type": "collection",
      "label": "Collection"
      },
      {
          "type": "url",
          "id": "btn_link",
          "label": "Link",
          "info": "Note: if you want to display this button then you need to fill button link"
        }
      ]
    },
    {
      "type": "cart",
      "name": "Cart",
      "limit":1
    },
    {
      "type": "search",
      "name": "Search",
      "limit":1
    },
    {
      "type": "account",
      "name": "Account",
      "limit":1
    },
    {
      "type": "wishlist",
      "name": "Wishlist",
      "limit":1
    }

  ]
}
{%- endschema -%}
